{
  "manifest_version": 3,
  "name": "WP-admin",
  "version": "1.0",
  "description": "This extension opens the WP Dashboard for the current site.",
  "author": "Terehov Sergey",
  "permissions": ["contextMenus", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}