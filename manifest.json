{
  "name": "WP Admin",
  "description": "Open WordPress dashboard in new tab",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {},
  "action": {},
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "main.js" ]
    }
  ]
}